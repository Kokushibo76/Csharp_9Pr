<Window x:Class="_9_praktos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_9_praktos"
        mc:Ignorable="d"
        Title="MainWindow" Height="444" Width="301" Background="AntiqueWhite" ResizeMode="NoResize">
    <Grid Margin="0,0,0,-6">
        <TextBlock Text="Добро пожаловать в редактор файлов C#" Margin="5,57,5,0" FontSize="15" VerticalAlignment="Top" Height="68" Foreground="DarkOrchid"/>
        <Button x:Name="WordCreate" Margin="10,187,10,163" Background="Purple" Foreground="Beige" Content="Работа с WORD документом" FontSize="20" Click="WordCreate_Click" />
        <Button x:Name="ExcelOpen" Margin="10,294,10,57" Background="Purple" Foreground="Beige" Content="Работа с Excel EXCEL документом" FontSize="20" Click="ExcelOpen_Click"/>
    </Grid>
</Window>
